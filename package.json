{
  "name": "taroify",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "jest",
    "core:watch": "babel -w packages/core/src/ -d bundles/core --copy-files --extensions \".ts,.tsx\"",
    "core-decorators:watch": "tsc -w -p packages/core/tsconfig.d.json",
    "icons:watch": "babel -w packages/icons/src/ -d bundles/icons --copy-files --extensions \".ts,.tsx\"",
    "icons-decorators:watch": "tsc -w -p packages/icons/tsconfig.d.json",
    "bundles:clean": "node scripts/clean.js"
  },
  "workspaces": [
    "packages/*",
    "bundles/*"
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.12.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^13.1.2",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "global-jsdom": "^8.0.0",
    "jest": "^26.6.0",
    "jest-transform-css": "^2.1.0",
    "lerna": "^3.22.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.1.3"
  }
}
