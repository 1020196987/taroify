{
  "name": "vant",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "jest": "jest",
    "dts": "tsc --help",
    "watch:components": "babel -w packages/components/src/ -d bundles/components --copy-files --extensions \".ts,.tsx\"",
    "watch:components-decorators:": "tsc -w -p packages/components/tsconfig.d.json",
    "watch:icons": "babel -w packages/icons/src/ -d bundles/icons --copy-files --extensions \".ts,.tsx\"",
    "watch:icons-decorators:": "tsc -w -p packages/icons/tsconfig.d.json"
  },
  "workspaces": [
    "packages/*",
    "bundles/*"
  ],
  "dependencies": {
    "@tarojs/components": "^3.0.26",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.12.13",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/react-test-renderer": "^17.0.0",
    "cross-env": "^7.0.3",
    "jest": "^26.6.0",
    "jest-transform-css": "^2.1.0",
    "lerna": "^3.22.1",
    "react-test-renderer": "^17.0.1",
    "typescript": "^4.1.3"
  }
}
